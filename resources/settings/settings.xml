<settings>
    <setting propertyKey="@Properties.Theme" title="@Strings.ThemeTitle">
        <settingConfig type="list">
            <listEntry value="0">@Strings.ColorWhite</listEntry>
            <listEntry value="1">@Strings.ColorRed</listEntry>
            <listEntry value="2">@Strings.ColorGreen</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.GraphStyle" title="@Strings.GraphStyleTitle">
        <settingConfig type="list">
            <listEntry value="0">@Strings.GraphLine</listEntry>
            <listEntry value="1">@Strings.GraphCandles</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.GraphType" title="@Strings.GraphTypeTitle">
        <settingConfig type="list">
            <listEntry value="0">@Strings.GraphFTX</listEntry>
            <listEntry value="1">@Strings.GraphStepHistory</listEntry>
        </settingConfig>
    </setting>


</settings>
